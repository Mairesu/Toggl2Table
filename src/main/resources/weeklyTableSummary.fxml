<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane prefHeight="90.0" prefWidth="529.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UnemployedVoodooFamily.GUI.Content.WeeklySummaryViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="60.0">
         <children>
            <Label fx:id="headerLabel" text="Weekly report">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox prefHeight="200.0" GridPane.columnIndex="1">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="60.0">
               <children>
                  <Label fx:id="weekNumLabel1" text="hours worked ">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="hoursWorkedLabel" text="0.0">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="60.0">
               <children>
                  <Label fx:id="weekNumLabel11" text="extraTime hours ">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="overtimeWorkedLabel" text="0.0">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" />
   </columnConstraints>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <rowConstraints>
      <RowConstraints maxHeight="70.0" minHeight="70.0" prefHeight="70.0" vgrow="SOMETIMES" />
   </rowConstraints>
</GridPane>
